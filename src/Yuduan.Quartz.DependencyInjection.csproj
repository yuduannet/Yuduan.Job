    <Copyright>Yuduan Network</Copyright>
    <PackageProjectUrl>https://github.com/yuduannet/Yuduan.Quartz.DependencyInjection</PackageProjectUrl>
    <RepositoryUrl>https://github.com/yuduannet/Yuduan.Quartz.DependencyInjection</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0" />
    <PackageReference Include="Quartz" Version="3.3.3" />
  </ItemGroup>

</Project>
